  "license": "MIT",
  "main": "dist/vue2-preloader.ssr.js",
  "browser": "dist/vue2-preloader.esm.js",
  "module": "dist/vue2-preloader.esm.js",
  "unpkg": "dist/vue2-preloader.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
